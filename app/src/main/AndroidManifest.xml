<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zybooks.gameemulator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_my_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DeletePlayerActivity"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".AddPlayerActivity"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".SelectPlayer2Activity"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".SelectPlayer1Activity"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".ScoreboardActivity"
            android:parentActivityName=".MainMenuActivity" />
        <activity
            android:name=".GameEmulatorActivity"
            android:parentActivityName=".MainMenuActivity" />
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>